{
  "name": "macrocoupon-web-react",
  "version": "1.0.0",
  "description": "Macrocoupon is a e-commerce website of selling products",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "build": "webpack -d && webpack-dev-server --content-base public/ --inline --hot --history-api-fallback",
    "build:prod": "webpack -p",
    "test": "mocha --recursive --compilers js:babel-register --require babel-polyfill --require ignore-styles",
    "test-watch": "npm test -- --watch"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/sethkong/macrocoupon-web-react.git"
  },
  "keywords": [
    "react",
    "react-bootstrap-autosuggest",
    "react-bootstrap",
    "react-redux",
    "redux-saga",
    "bootstrap",
    "combobox",
    "datalist",
    "autocomplete",
    "autosuggest",
    "webpack"
  ],
  "author": "Team Macrocoupon",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/sethkong/macrocoupon-web-react/issues"
  },
  "homepage": "https://gitlab.com/sethkong/macrocoupon-web-react#README",
  "dependencies": {
    "axios": "^0.15.3",
    "jquery": "^3.1.1",
    "material-ui": "^0.16.5",
    "react": "^15.4.1",
    "react-autosuggest": "^7.0.1",
    "react-bootstrap": "^0.30.7",
    "react-bootstrap-autosuggest": "^0.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-bootstrap": "^0.23.1",
    "react-router-redux": "^4.0.7",
    "react-scroll": "^1.4.4",
    "react-simple-dropdown": "^1.1.5",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-form": "^6.3.2",
    "redux-form-validation": "0.0.8",
    "redux-saga": "^0.13.0"
  },
  "devDependencies": {
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.9",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-runtime": "^6.20.0",
    "clean-webpack-plugin": "^0.1.14",
    "css-loader": "^0.26.1",
    "enzyme": "^2.6.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "http-server": "^0.9.0",
    "ignore-styles": "^5.0.1",
    "mocha": "^3.2.0",
    "node-sass": "^3.13.0",
    "postcss-loader": "^1.2.1",
    "react-addons-test-utils": "^15.4.1",
    "react-bootstrap-autosuggest": "^0.4.1",
    "react-hot-loader": "^1.3.1",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.13.2"
  }
}
